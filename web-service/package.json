{
    "name": "sentiment-analysis-web",
    "version": "1.0.0",
    "description": "Node.js/Express web server for sentiment analysis with FastAPI microservice backend",
    "main": "dist/server.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/server.js",
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "watch": "tsc --watch"
    },
    "dependencies": {
        "axios": "^1.6.0",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "helmet": "^7.1.0",
        "morgan": "^1.10.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.19",
        "@types/express": "^4.17.23",
        "@types/morgan": "^1.9.10",
        "@types/node": "^20.19.19",
        "nodemon": "^3.0.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.9.3"
    },
    "keywords": [
        "sentiment-analysis",
        "express",
        "fastapi",
        "microservices"
    ],
    "author": "",
    "license": "MIT"
}